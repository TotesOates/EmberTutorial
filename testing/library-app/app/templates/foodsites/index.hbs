<h2>List</h2>
{{#each model as |food|}}
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">{{food.name}}</h3>
    </div>
    <div class="panel-body">
      <p>Address: {{food.address}}</p>
      <p>Phone: {{food.phone}}</p>
    </div>
  </div>
{{/each}}